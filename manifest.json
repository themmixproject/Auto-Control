{
    "name": "Auto Control",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "icons": {
        "16": "icons/autocontrol16x16.png",
        "32": "icons/autocontrol32x32.png",
        "48": "icons/autocontrol48x48.png",
        "128": "icons/autocontrol128x128.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/ui.js", "js/content.js", "js/elementCard.js"],
            "css": ["css/content.css", "css/element-list-item.css"]
        }
    ],
    "action": {},
    "background": {
        "service_worker": "background.js"
    },
    "web_accessible_resources": [
        {
            "matches": ["<all_urls>"],
            "resources": ["assets/cross.svg"]
        }
    ]
}
